  font-size: 12px
}

#toggle .option {
  display: inline-block;
  background: rgb(12, 248, 236);
  line-height: 1;
  padding: 5px 10px;
  -webkit-transition: .3s;
  transition: .3s
}

#toggle .option.selected {
  color: rgb(201, 12, 248)
}

#toggle .hb {
  box-sizing: initial;
  content: "";
  position: absolute;
  top: 2px;
  left: 2px;
  width: 4em;
  height: 1em;
  border-radius: 20px;
  background: #00bdd6;
  padding: 5px 10px;
  -webkit-transition: .3s;
  transition: .3s;
  z-index: -1
}

#toggle .hb.monster {
